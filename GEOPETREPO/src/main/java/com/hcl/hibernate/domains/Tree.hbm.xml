<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hcl.hibernate.domains.TreeDO" table="TREE">
		<id name="treeNo" type="java.lang.Integer">
			<column name="TREENO" />
			<generator class="assigned" />
		</id>
		<property name="treeName" type="java.lang.String">
			<column name="TREENAME" length="50" not-null="true" />
		</property>
	</class>
</hibernate-mapping>